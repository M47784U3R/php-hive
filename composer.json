{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "ministryofcode/php-hive",
  "description": "PHP Hive allows running PHP processes in parallel to reduce execution time",
  "type": "library",
  "keywords": ["parallel", "library", "multi-core", "multi-thread"],
  "license": "GPL-3.0-only",
  "autoload": {
    "psr-4": {
      "MinistryOfCode\\Hive\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "ramsey/uuid": "^4.9",
    "katzgrau/klogger": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5.55"
  }
}
